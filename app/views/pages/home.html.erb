<script type="text/javascript">
	$("#mailingListForm input[type=submit]").on('click', function(e){
		e.preventDefault();
		var mailListEmail = $("#mailingListEmail").val();
		
		if(mailListEmail ==""){
			alert("Please enter a valid email address.")
		} else {
			$("#mailingListForm").submit();
			alert("Thank you! We will reach out to you shortly.");
		}
	});
	$("#mailingListEmail").on("blur", function(e){
		e.preventDefault();
		var mailListEmail = $(e.target).val();
		
		if(mailListEmail !=""){
			$("#mailingListForm").submit();
			alert("Thank you! We will reach out to you shortly.");
		}
	})
</script>
